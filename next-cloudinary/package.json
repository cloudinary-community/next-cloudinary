    "postpublish": "rm ./README.md",
    "test": "jest"
  },
  "dependencies": {
    "@cloudinary/url-gen": "^1.8.5"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@types/jest": "^29.1.1",
    "babel-jest": "^29.1.2",
    "jest": "^29.1.2",
    "jest-environment-jsdom": "^29.1.2",
    "microbundle": "^0.15.1"
  },
  "peerDependencies": {
    "next": "^12.2.5",
    "react": "^18.2.0"
  }
}
