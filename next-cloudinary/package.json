{
  "name": "next-cloudinary",
  "version": "6.5.2",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "cp ../README.md . && cp ../LICENSE . && pnpm build",
    "postpublish": "rm ./README.md && rm ./LICENSE",
    "test": "vitest run",
    "test:app": "NEXT_PUBLIC_CLOUDINARY_CLOUD_NAME=\"test\" pnpm build && cd tests/nextjs-app && npm install && npm run build",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@cloudinary-util/types": "1.0.5",
    "@cloudinary-util/url-loader": "5.3.0",
    "@cloudinary-util/util": "^3.0.0",
    "@tsconfig/recommended": "^1.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.33",
    "@types/react-dom": "^18.2.14",
    "dotenv": "^16.3.1",
    "mkdirp": "^3.0.1",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "next": "^12 || ^13 || ^14",
    "react": "^17 || ^18"
  }
}
